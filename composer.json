{
    "name": "webkul/graphql-channel-extension",
    "description": "GraphQL Channel Extension for Bagisto - Adds channel support to GraphQL API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Webkul",
            "email": "support@webkul.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\GraphQLChannelExtension\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\GraphQLChannelExtension\\Providers\\GraphQLChannelExtensionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
